                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/perception" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="20dp"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/knowledgeAvailability"
                        android:layout_width="15dp"
                        android:layout_height="wrap_content"
                        android:text="@string/knowledgeAvailability" />
                    <TextView
                        android:id="@+id/knowledgeComment"
                        android:layout_width="150dp"
                        android:layout_height="wrap_content"
                        android:text="@string/knowledgeComment" />

                    <TextView
                        android:id="@+id/knowledge"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/knowledge" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="20dp"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/survivalAvailability"
                        android:layout_width="15dp"
                        android:layout_height="wrap_content"
                        android:text="@string/survivalAvailability" />
                    <TextView
                        android:id="@+id/survivalComment"
                        android:layout_width="150dp"
                        android:layout_height="wrap_content"
                        android:text="@string/survivalComment" />

                    <TextView
                        android:id="@+id/survival"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/survival" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="20dp"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/insightAvailability"
                        android:layout_width="15dp"
                        android:layout_height="wrap_content"
                        android:text="@string/insightAvailability" />
                    <TextView
                        android:id="@+id/insightComment"
                        android:layout_width="150dp"
                        android:layout_height="wrap_content"
                        android:text="@string/insightComment" />

                    <TextView
                        android:id="@+id/insight"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/insight" />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="130dp"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/empathyComment"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/empathyComment" />

                <TextView
                    android:id="@+id/empathy"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/empathy" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="20dp"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/animalHandlingAvailability"
                        android:layout_width="15dp"
                        android:layout_height="wrap_content"
                        android:text="@string/animalHandlingAvailability" />
                    <TextView
                        android:id="@+id/animalHandlingComment"
                        android:layout_width="200dp"
                        android:layout_height="wrap_content"
                        android:text="@string/animalHandlingComment" />

                    <TextView
                        android:id="@+id/animalHandling"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/animalHandling" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="20dp"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/medicineAvailability"
                        android:layout_width="15dp"
                        android:layout_height="wrap_content"
                        android:text="@string/medicineAvailability" />
                    <TextView
                        android:id="@+id/medicineComment"
                        android:layout_width="150dp"
                        android:layout_height="wrap_content"
                        android:text="@string/medicineComment" />

                    <TextView
                        android:id="@+id/medicine"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/medicine" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="20dp"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/manipulationAvailability"
                        android:layout_width="15dp"
                        android:layout_height="wrap_content"
                        android:text="@string/manipulationAvailability" />
                    <TextView
                        android:id="@+id/manipulationComment"
                        android:layout_width="150dp"
                        android:layout_height="wrap_content"
                        android:text="@string/manipulationComment" />

                    <TextView
                        android:id="@+id/manipulation"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/manipulation" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="20dp"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/performanceAvailability"
                        android:layout_width="15dp"
                        android:layout_height="wrap_content"
                        android:text="@string/performanceAvailability" />
                    <TextView
                        android:id="@+id/performanceComment"
                        android:layout_width="150dp"
                        android:layout_height="wrap_content"
                        android:text="@string/performanceComment" />

                    <TextView
                        android:id="@+id/performance"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/performance" />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">

            <LinearLayout
                android:layout_width="130dp"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/hygieneComment"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/hygieneComment" />

                <TextView
                    android:id="@+id/hygiene"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/hygiene" />

            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/talantComment"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/talantComment" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/nameTalant"
                        android:layout_width="150dp"
                        android:layout_height="wrap_content"
                        android:text="@string/nameTalant" />
                    <TextView
                        android:id="@+id/levelTalant"
                        android:layout_width="150dp"
                        android:layout_height="wrap_content"
                        android:text="@string/levelTalant" />
                </LinearLayout>



            </LinearLayout>
        </LinearLayout>

    </LinearLayout>



</ScrollView>